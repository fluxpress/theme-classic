<%- include('../_partials/navbar', { siteTitle, currentNav: 'categories' }) %>

<main>
  <section>
    <div class="type-cloud">
      <% categories.forEach(category => { %>
        <a class="button button-outline button-large button-no-transform" href="/categories/<%= category.id %>"><%= category.title %></a>
      <% }) %>
    </div>
  </section>
</main>
